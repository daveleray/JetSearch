<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

<servlet>
		<servlet-name>CloudReviewerServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.CloudReviewerServlet</servlet-class>
	</servlet>
	

		<servlet>
		<servlet-name>LogOutURLServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.LogOutURLServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>GoogleDriveAuthServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.GoogleDriveAuthServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>TestServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.TestServlet</servlet-class>
	</servlet>
	
		<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/test</url-pattern>
		</servlet-mapping>
	
				<servlet>
		<servlet-name>BatchServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.BatchServlet</servlet-class>
	</servlet>
	
		<servlet-mapping>
		<servlet-name>BatchServlet</servlet-name>
		<url-pattern>/batches</url-pattern>
		</servlet-mapping>
		
		
		
		
				<servlet-mapping>
		<servlet-name>GoogleDriveAuthServlet</servlet-name>
		<url-pattern>/googledriveauth</url-pattern>
	</servlet-mapping>
	
			<servlet-mapping>
		<servlet-name>CloudReviewerServlet</servlet-name>
		<url-pattern>/cloudreviewer</url-pattern>
	</servlet-mapping>
	
			<servlet>
		<servlet-name>ApplyTagServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.TagServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>ApplyTagServlet</servlet-name>
		<url-pattern>/applytag</url-pattern>
		</servlet-mapping>
		
					<servlet>
		<servlet-name>FolderServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.FolderServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>FolderServlet</servlet-name>
		<url-pattern>/folders</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>DocMetadataServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.DocMetadataServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>DocMetadataServlet</servlet-name>
		<url-pattern>/metadata</url-pattern>
		</servlet-mapping>
		
		
				<servlet>
		<servlet-name>CSVServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.CSVServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>CSVServlet</servlet-name>
		<url-pattern>/getcsv</url-pattern>
		</servlet-mapping>
		
		
				<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.UserServlet</servlet-class>
	</servlet>
	
		<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/usersetup</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>PanelServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.PanelServlet</servlet-class>
	</servlet>
	
		<servlet-mapping>
		<servlet-name>PanelServlet</servlet-name>
		<url-pattern>/panel</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
		<servlet-name>GetDocServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.DocServlet</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>GetDocServlet</servlet-name>
		<url-pattern>/doc</url-pattern>
		</servlet-mapping>
	
		
			<servlet>
		<servlet-name>MetadataWorker</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.MetadataWorker</servlet-class>
		</servlet>
		<servlet-mapping>
		<servlet-name>MetadataWorker</servlet-name>
		<url-pattern>/tasks/metadataworker</url-pattern>
		</servlet-mapping>
		
		<security-constraint>
    <web-resource-collection>
        <web-resource-name>tasks</web-resource-name>
        <url-pattern>/tasks/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
        <role-name>admin</role-name>
    </auth-constraint>
</security-constraint>

		
	
		<servlet>
		<servlet-name>PDFServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.PDFServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>PDFServlet</servlet-name>
		<url-pattern>/getpdf</url-pattern>
		</servlet-mapping>
		
		<servlet>
		<servlet-name>TextServlet</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.TextServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>TextServlet</servlet-name>
		<url-pattern>/gettext</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
		<servlet-name>Sync</servlet-name>
		<servlet-class>com.dleray.cloudreviewer.GoogleDriveSync</servlet-class>
		</servlet>
		
		<servlet-mapping>
		<servlet-name>Sync</servlet-name>
		<url-pattern>/sync</url-pattern>
		</servlet-mapping>
		
	
		<servlet-mapping>
		<servlet-name>LogOutURLServlet</servlet-name>
		<url-pattern>/getlo</url-pattern>
	</servlet-mapping>
	

	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	

    
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value>com.dleray.cloudreviewer.endpoints.BatchFolderEndpoint,com.dleray.cloudreviewer.endpoints.CloudReviewerUserEndpoint,com.dleray.cloudreviewer.endpoints.DocumentBatchEndpoint,com.dleray.cloudreviewer.endpoints.DocumentEndpoint,com.dleray.cloudreviewer.endpoints.DocumentMetadataEndpoint,com.dleray.cloudreviewer.endpoints.IssueTagEndpoint,com.dleray.cloudreviewer.endpoints.TagListEndpoint,com.dleray.cloudreviewer.endpoints.TaggingPanelEndpoint,com.dleray.cloudreviewer.endpoints.UserTagEndpoint</param-value>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>